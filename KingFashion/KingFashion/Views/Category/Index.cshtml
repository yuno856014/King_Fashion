@model IEnumerable<KingFashion.Models.Categorys.CategoryView>

@{
    ViewData["Title"] = "Index";
}
<p>
    <a class="btn btn-out btn-success btn-square" asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                <strong>#</strong>
            </th>
            <th>
                Danh Mục
            </th>
            <th>
                Trạng thái
            </th>
            <th>
                Bị Xóa
            </th>
            <th>
                Thể Loại
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CatDetailsId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CatDetailsName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsDeleted)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CategoryName)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })
            </td>
        </tr>
}
    </tbody>
</table>
